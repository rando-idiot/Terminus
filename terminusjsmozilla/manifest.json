{
  "manifest_version": 2,
  "name": "TerminusJS",
  "version": "6.0",

  "description": "Adds a small game to play in mozilla.org on the terminal.",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": [ "*://*.mozilla.org/*" ],
      "js": [ "terminus.js" ]
    }
  ]
}
